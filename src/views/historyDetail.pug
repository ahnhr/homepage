extends layouts/main

block content
    .history__content
        h5.subpage__name history
        h3.subpage__title 주요 연혁
        h5.history__text-title=history.title
        h3.history__box-date=history.date             
        p.history__text-info=history.description
        img(src=`/${history.fileUrl}`)
        if loggedUser
            a(href=routes.editHistory(history.id)) Edit History